{
  "name": "pixelshapevts",
  "version": "0.0.2",
  "description": "Pixel editor that comes in handy when creating pixel art images and animations",
  "main": "./electron/renderer.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "prestart": "npm run clean",
    "start": "electron-forge start",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lint": "eslint ./src",
    "test-runner": "babel-node node_modules/blue-tape/bin/blue-tape -r ./test/requireHook.js test/**/*.spec.js",
    "pretest": "npm run lint",
    "test": "npm run test-runner",
    "test:dev": "npm run test-runner | tap-spec || true",
    "coverage": "cross-env NODE_ENV=test nyc tape -r ./test/requireHook.js -- test/**/*.spec.js | tap-spec",
    "postcoverage": "nyc report --reporter=html",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Renpona/PixelShapeVTS"
  },
  "bugs": {
    "url": "https://github.com/Renpona/PixelShapeVTS/issues"
  },
  "homepage": "https://github.com/Renpona/PixelShapeVTS#readme",
  "keywords": [
    "pixel",
    "artwork",
    "editor",
    "online",
    "8bit"
  ],
  "author": "Renpona",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.66",
    "@electron-forge/maker-deb": "^6.0.0-beta.66",
    "@electron-forge/maker-rpm": "^6.0.0-beta.66",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.66",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@eleks/eslint-config-eleks": "^0.1.1",
    "atob": "^2.0.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.23.0",
    "blue-tape": "^1.0.0",
    "classnames": "^2.2.5",
    "coveralls": "^2.12.0",
    "cross-env": "^2.0.1",
    "css-loader": "^3.6.0",
    "electron": "^19.0.15",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "exports-loader": "^0.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "File": "^0.10.2",
    "file-loader": "^5.1.0",
    "filereader": "^0.10.3",
    "html-webpack-plugin": "^4.5.2",
    "imports-loader": "^0.8.0",
    "nyc": "^10.1.2",
    "open": "0.0.5",
    "redux-logger": "^2.6.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "tap-spec": "^4.1.1",
    "tiny-worker": "^2.0.3",
    "url-loader": "^3.0.0",
    "w3c-blob": "0.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1",
    "worker-loader": "^3.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "electron-squirrel-startup": "^1.0.0",
    "es6-promise": "^4.1.0",
    "file-saver": "^1.3.2",
    "hyperscript": "^2.0.2",
    "jszip": "^3.1.3",
    "keyboardjs": "^2.3.3",
    "pako": "^1.0.5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "redux-undo": "^1.0.0-beta9-7",
    "vtubestudio": "^1.11.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pixelshapevts"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
